<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fitsSystemWindows="true"
    tools:context=".ui.EditarPerfil.EditarPerfilActivity">

    <com.google.android.material.appbar.AppBarLayout
        android:id="@+id/app_bar"
        android:layout_width="match_parent"
        android:fitsSystemWindows="true"
        android:layout_height="100dp"
        android:theme="@style/Theme.AppBibliotecas.AppBarOverlay">

        <androidx.appcompat.widget.Toolbar
            android:id="@+id/toolbar_editarperfil"
            android:layout_height="?attr/actionBarSize"
            app:menu="@menu/menu_editar_perfil"
            android:layout_width="match_parent"
            app:layout_collapseMode="parallax"
            android:backgroundTint="@color/white"
            app:popupTheme="@style/Theme.AppBibliotecas.PopupOverlay" />
    </com.google.android.material.appbar.AppBarLayout>

    <include
        android:id="@+id/editarperfil_layout"
        layout="@layout/editarperfil_contentscrolling" />



</androidx.coordinatorlayout.widget.CoordinatorLayout>